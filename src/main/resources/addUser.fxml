<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXDatePicker?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXSpinner?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane id="AddUser" prefHeight="768.0" prefWidth="1366.0"
	styleClass="addUser" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="carmo.tiago.ui.AddUserController">
	<Group>
		<children>
			<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
				prefHeight="768.0" prefWidth="1366.0">
				<Label fx:id="successMessage" alignment="CENTER"
					contentDisplay="CENTER" layoutX="100.0" layoutY="120.0" opacity="0.0"
					prefHeight="25.0" prefWidth="200.0" textAlignment="CENTER"
					textFill="#0091f8">
					<effect>
						<DropShadow />
					</effect>
				</Label>
				<Label fx:id="errorMessage" alignment="CENTER" contentDisplay="CENTER"
					layoutX="100.0" layoutY="120.0" opacity="0.0" prefHeight="25.0"
					prefWidth="200.0" textAlignment="CENTER" textFill="RED">
					<effect>
						<DropShadow />
					</effect>
				</Label>
				<Label fx:id="nameErrorMessage" alignment="CENTER_RIGHT"
					contentDisplay="RIGHT" layoutX="414.0" layoutY="204.0" prefHeight="17.0"
					prefWidth="160.0" textAlignment="CENTER" textFill="RED">
					<effect>
						<DropShadow />
					</effect>
				</Label>
				<Label fx:id="emailErrorMessage" alignment="CENTER_RIGHT"
					contentDisplay="RIGHT" layoutX="414.0" layoutY="254.0" prefHeight="17.0"
					prefWidth="160.0" textAlignment="CENTER" textFill="RED">
					<effect>
						<DropShadow />
					</effect>
				</Label>
				<Label fx:id="passwordErrorMessage" alignment="CENTER_RIGHT"
					contentDisplay="RIGHT" layoutX="414.0" layoutY="304.0" prefHeight="17.0"
					prefWidth="160.0" textAlignment="CENTER" textFill="RED">
					<effect>
						<DropShadow />
					</effect>
				</Label>
				<Label fx:id="password2ErrorMessage" alignment="CENTER_RIGHT"
					contentDisplay="RIGHT" layoutX="414.0" layoutY="354.0" prefHeight="17.0"
					prefWidth="160.0" textAlignment="CENTER" textFill="RED">
					<effect>
						<DropShadow />
					</effect>
				</Label>
				<JFXButton fx:id="confirmAddUser" alignment="CENTER"
					buttonType="RAISED" contentDisplay="CENTER" defaultButton="true"
					layoutX="583.0" layoutY="550.0" onAction="#processAddUser"
					prefHeight="25.0" prefWidth="200.0" ripplerFill="BLACK" text="CONFIRM"
					textAlignment="CENTER" />
				<JFXButton fx:id="backAddUser" alignment="CENTER"
					buttonType="RAISED" contentDisplay="CENTER" layoutX="583.0"
					layoutY="600.0" onAction="#goBack" prefHeight="25.0" prefWidth="200.0"
					ripplerFill="BLACK" text="BACK" />
				<JFXTextField id="nameAddUser" fx:id="name" focusColor="TURQUOISE"
					layoutX="583.0" layoutY="200.0" prefHeight="25.0" prefWidth="200.0"
					promptText="Name" stylesheets="@demo.css" />
				<JFXTextField id="emailAddUser" fx:id="email"
					focusColor="TURQUOISE" layoutX="584.0" layoutY="250.0" prefHeight="25.0"
					prefWidth="200.0" promptText="Email" stylesheets="@demo.css" />
				<JFXPasswordField id="passwordAddUser" fx:id="password"
					focusColor="TURQUOISE" layoutX="584.0" layoutY="300.0" prefHeight="25.0"
					prefWidth="200.0" promptText="Password" stylesheets="@demo.css" />
				<JFXPasswordField id="password2AddUser" fx:id="password2"
					focusColor="TURQUOISE" layoutX="583.0" layoutY="350.0" prefHeight="25.0"
					prefWidth="200.0" promptText="Retype password" stylesheets="@demo.css" />
				<JFXTextField id="weightAddUser" fx:id="weight"
					focusColor="TURQUOISE" layoutX="584.0" layoutY="450.0" prefHeight="25.0"
					prefWidth="78.0" promptText="Weight (kg)" />
				<JFXTextField id="heightAddUser" fx:id="height"
					focusColor="TURQUOISE" layoutX="676.0" layoutY="450.0" prefHeight="25.0"
					prefWidth="111.0" promptText="Height (cm)" />
				<JFXComboBox id="sexAddUser" fx:id="sex" focusColor="TURQUOISE"
					layoutX="583.0" layoutY="500.0" prefHeight="25.0" prefWidth="78.0"
					promptText="Sex" stylesheets="@demo.css" />
				<JFXComboBox id="activityLevelAddUser" fx:id="activityLevel"
					focusColor="TURQUOISE" layoutX="676.0" layoutY="500.0" prefHeight="25.0"
					prefWidth="111.0" promptText="Activity level" stylesheets="@demo.css" />
				<JFXDatePicker id="AgePickerAddUser" fx:id="agePicker"
					defaultColor="TURQUOISE" layoutX="583.0" layoutY="400.0"
					prefHeight="28.0" prefWidth="200.0" promptText="Date of birth (YYYY-MM-dd)" />
				<JFXSpinner id="spinnerAddUser" fx:id="spinner" layoutX="200.0"
					layoutY="171.0" opacity="0.0" />
			</AnchorPane>
		</children>
	</Group>
	<stylesheets>
		<URL value="@demo.css" />
	</stylesheets>
</StackPane>
