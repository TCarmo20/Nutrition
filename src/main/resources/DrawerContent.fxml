<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="anchorPaneBar" fx:id="anchorPane" prefHeight="600.0" prefWidth="200.0" stylesheets="@demo.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carmo.tiago.ui.DrawerContentController">
   <children>
      <VBox fx:id="vBoxMenuBar" alignment="TOP_CENTER" layoutX="16.0" prefWidth="200.0" stylesheets="@demo.css" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      	<children>
      		<ImageView fx:id="imageViewMenuBar" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
      			<image>
      				<Image url="@Pictures/preloaderWall.jpg" />
      			</image>
      		</ImageView>
      		<JFXButton fx:id="CreatePlanMenuBar" accessibleText="Create Plan" contentDisplay="CENTER" prefHeight="35.0" prefWidth="198.0" ripplerFill="WHITE" text="CREATE PLAN" />
      		<JFXButton fx:id="MyPlansMenuBar" accessibleText="My Plans" contentDisplay="CENTER" prefHeight="35.0" prefWidth="198.0" ripplerFill="WHITE" text="MY PLANS" />
      		<JFXButton fx:id="UpdateDetailsMenuBar" accessibleText="Update Details" contentDisplay="CENTER" prefHeight="35.0" prefWidth="198.0" ripplerFill="WHITE" text="UPDATE DETAILS" />
      		<JFXButton fx:id="LogoutManuBar" accessibleText="Logout" contentDisplay="CENTER" prefHeight="35.0" prefWidth="198.0" ripplerFill="WHITE" text="LOGOUT" />
      	</children>
      </VBox>
      <Label fx:id="nameLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="41.0" prefWidth="200.0" textAlignment="CENTER" textFill="WHITE" wrapText="true" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Label>
   </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
