<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="carmo.tiago.ui.HomePageController">
	<children>
		<Group>
			<children>
				<AnchorPane id="HomePage" prefHeight="768.0" prefWidth="1366.0" styleClass="homepage">
					<Button fx:id="update" layoutX="387.0" layoutY="372.0" maxHeight="2.0" onAction="#processUpdate" text="Update" />
					<Label fx:id="message" layoutX="56.0" layoutY="77.0" text="Please review your profile data." />
					<Label layoutX="56.0" layoutY="123.0" text="User:" />
					<Label layoutX="56.0" layoutY="173.0" text="Email:" />
					<TextField fx:id="user" editable="false" layoutX="149.0" layoutY="120.0" maxWidth="2.0" minHeight="30.0" minWidth="215.0" prefHeight="30.0" prefWidth="215.0" />
					<TextField fx:id="email" layoutX="149.0" layoutY="171.0" maxWidth="2.0" minHeight="30.0" minWidth="215.0" prefHeight="30.0" prefWidth="215.0" />
					<Hyperlink fx:id="logout" layoutY="24.0" onAction="#processLogout" text="logout" AnchorPane.rightAnchor="52.0" />
					<Button fx:id="Button" defaultButton="true" disable="true" layoutX="481.0" layoutY="372.0" maxHeight="2.0" maxWidth="100.0" minWidth="100.0" onAction="#processLogout" prefWidth="100.0" text="Continue" />
