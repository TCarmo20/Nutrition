					<Label fx:id="error" layoutX="28.0" layoutY="464.0" opacity="0.0" text="Error updating profile!" />
					<TextField fx:id="name" layoutX="149.0" layoutY="222.0" maxWidth="2.0" minHeight="30.0" minWidth="215.0" prefHeight="30.0" prefWidth="215.0" />
					<Label layoutX="56.0" layoutY="225.0" text="Name:" />
					<stylesheets>
						<URL value="@demo.css" />
					</stylesheets>
				</AnchorPane>
			</children>
		</Group>
	</children>
</StackPane>
